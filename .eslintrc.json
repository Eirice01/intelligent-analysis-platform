{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react-hooks"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "rules": {
    "no-unused-vars": [1, {
      "args": "none",
      "ignoreRestSiblings": true,
      "caughtErrors": "none"
    }],
    "eqeqeq": 0,
    "no-console": 0,
    "react-hooks/rules-of-hooks": 1,
    "react/prop-types": 0,
    "react/display-name": 0
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
